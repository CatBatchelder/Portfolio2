<section class="content-spacing" id="contact-title">
    <h1 class="title-text">Contact</h1>
</section>

<section class=" grid-container" id="contact-form">
    <div>
        <div>
            <!-- <img src="img/contact/headshot.png" alt="Catherine Batchelder Headshot"> -->
            <h3 class="content-text">Feel free to contact me on the form below or through my email at: <a
                    href="mailto:catherinemaebatchelder@gmail.com">catherinemaebatchelder@gmail.com</a> for any inquiries.</h3>

        </div>


        <form action="https://formspree.io/xgenplrz" method="POST">
            <section>
                <input type="text" placeholder="NAME" name="Name" id="contact-name">
                <input type="email" placeholder="EMAIL" name="Email" id="contact-email">
                <textarea  placeholder="COMMENTS" name="Comments" id="contact-comments"></textarea>

                <button id="contact-submit" class="pink-button" type="submit">SUBMIT</button>
                
            </section>

        </form>
    </div>
</section>

